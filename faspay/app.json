{
  "expo": {
    "name": "faspay",
    "slug": "faspay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme" : "https",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
	"favicon": "./assets/favicon.png",
	"bundler": "metro"
    },
    "experiments": {
      "baseUrl": "/faspay-app"
    },  
    "plugins": [
      "expo-router"
    ]
  }
}
